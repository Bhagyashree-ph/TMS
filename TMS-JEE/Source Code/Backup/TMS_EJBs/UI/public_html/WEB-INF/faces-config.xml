<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
  </application>
  <managed-bean>
    <managed-bean-name>admin</managed-bean-name>
    <managed-bean-class>ui.beans.AdminBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/viewuser.jsp</from-view-id>
    <navigation-case>
      <from-outcome>edit</from-outcome>
      <to-view-id>/updateuser.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/viewrole.jsp</from-view-id>
    <navigation-case>
      <from-outcome>edit</from-outcome>
      <to-view-id>/updaterole.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>log</managed-bean-name>
    <managed-bean-class>ui.beans.login</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>cop</managed-bean-name>
    <managed-bean-class>ui.beans.TrafficCop</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>rto</managed-bean-name>
    <managed-bean-class>ui.beans.RTOBean</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
    <from-view-id>/addOwner.jsp</from-view-id>
    <navigation-case>
      <from-outcome>addveh</from-outcome>
      <to-view-id>/addVehicle.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>own</from-outcome>
      <to-view-id>/addOwner.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>addreg</from-outcome>
      <to-view-id>/addRegistration.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/addVehicle.jsp</from-view-id>
    <navigation-case>
      <from-outcome>own</from-outcome>
      <to-view-id>/addOwner.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>addveh</from-outcome>
      <to-view-id>/addVehicle.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>addreg</from-outcome>
      <to-view-id>/addRegistration.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/addRegistration.jsp</from-view-id>
    <navigation-case>
      <from-outcome>own</from-outcome>
      <to-view-id>/addOwner.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>veh</from-outcome>
      <to-view-id>/addVehicle.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>addreg</from-outcome>
      <to-view-id>/addRegistration.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/Transfer.jsp</from-view-id>
    <navigation-case>
      <from-outcome>Success</from-outcome>
      <to-view-id>/Transfer2.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>Failure</from-outcome>
      <to-view-id>/Transfer.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/login.jsp</from-view-id>
    <navigation-case>
      <from-outcome>admin</from-outcome>
      <to-view-id>/admin.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>cop</from-outcome>
      <to-view-id>/copHome.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>clerk</from-outcome>
      <to-view-id>/clerkHome.jsp</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>rto</from-outcome>
      <to-view-id>/RTOHome.jsp</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>